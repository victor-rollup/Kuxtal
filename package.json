{
  "name": "kuxtal",
  "version": "0.0.0",
  "private": false,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "main": "dist/kuxtal.umd.js",
  "module": "dist/kuxtal.es.js",
  "types": "dist/index.d.ts",
  "author": {
    "name": "Cat Dog Studios"
  },
